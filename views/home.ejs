<%- include('partials/header'); -%>



<h1>Home</h1>
<%# rending text replacement from variables in app.js files %>
<p><%=homeStartingContent%></p>



<%# //for each loop, can also be substituted with for loop  %>
<%posts.forEach(function(post){%>
  <h1><%=post.title%></h1>
  <%# needs a bit of hint on external linking  %>
  <p><%=post.body.substring(0,300)+ "..." %></p>
  <a href="/posts/<%=post.title%>">Read More</a>
<%});%>




<%- include('partials/footer'); -%>
